public int maxProfit(int[] prices) {
        if(prices == null || prices.length == 0) return 0;
        int low =prices[0];
        int global =0;
        for(int i=1;i<prices.length;i++){
            int profit = prices[i]-low;
            if(profit>global){
                global = profit;
            }
            if(low>prices[i]){
                low= prices[i];
            }
        }
        return global;
    }
